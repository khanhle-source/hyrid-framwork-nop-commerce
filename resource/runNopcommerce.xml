<?xml version="1.0" encoding="UTF-8"?>
<suite name="NopCommerce">
    <parameter name = "environment" value="https://demo.nopcommerce.com/" />
    <test name="Run on Chrome">
        <parameter name = "browser" value="chrome" />
        <classes>
            <class name ="common.Common_Register_User"></class>
            <class name="com.nopcommerce.user.Level17_Close_Browser_Driver"></class>

        </classes>
    </test>

    <test name="Run on Firefox">
        <parameter name = "browser" value="firefox" />
        <classes>
            <class name ="common.Common_Register_User"></class>
            <class name="com.nopcommerce.user.Level16_Share_Data_User_Account"></class>
        </classes>
    </test>

</suite>